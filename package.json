{
  "name": "yomilink",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "NODE_ENV='development' NODE_OPTIONS='--import ./instrument.server.mjs' portless yomilink vite dev",
    "spotlight": "npx @spotlightjs/spotlight",
    "start": "node --import ./.output/server/instrument.server.mjs .output/server/index.mjs",
    "build": "vite build && cp instrument.server.mjs .output/server",
    "serve": "vite preview",
    "test": "vitest run -c vitest.ui.config.ts && vitest run -c vitest.service.config.ts",
    "test:ui": "vitest run -c vitest.ui.config.ts",
    "test:service": "vitest run -c vitest.service.config.ts",
    "test:watch": "vitest run -c vitest.ui.config.ts --watch",
    "test:ui:watch": "vitest run -c vitest.ui.config.ts --watch",
    "test:service:watch": "vitest run -c vitest.service.config.ts --watch",
    "coverage": "vitest run --coverage",
    "coverage:ui": "vitest run --coverage --coverage.ui",
    "format": "oxfmt --write ./src/**/*",
    "lint": "oxlint --fix ./src/**/*",
    "typecheck": "tsgo --noEmit",
    "cf-typegen": "wrangler types",
    "deploy": "wrangler deploy",
    "seed": "bun run --env-file=.env scripts/seed.ts",
    "ui": "bunx shadcn@canary",
    "effect:prepare": "effect-language-service patch"
  },
